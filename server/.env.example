# PostgreSQL Connection
DATABASE_URL=postgresql://postgres@localhost/twaine

# Session Secret (generate a strong random string)
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# Session Expiration (in milliseconds, default: 7 days)
SESSION_MAX_AGE=604800000

# Server Port
PORT=3001

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Node Environment
NODE_ENV=development

# Flux API Configuration (for image generation)
FLUX_API_URL=https://image.golinelli.ai
FLUX_API_KEY=your-flux-api-key-here
